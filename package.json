{
  "name": "token-gated",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "migrate:edgedb": "edgedb migrate && edgedb migration create",
    "generate:edgedb": "bunx @edgedb/generate edgeql-js --target ts",
    "generate:oazapfts:opensea:v2": "oazapfts ./src/lib/opensea/v2.openapi.json ./src/lib/opensea/v2.ts --optimistic --useEnumType"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.4.1",
    "@playwright/test": "^1.28.1",
    "@sveltejs/adapter-auto": "^2.0.0",
    "@sveltejs/kit": "^1.27.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-svelte": "^2.30.0",
    "prettier": "^3.0.0",
    "prettier-plugin-svelte": "^3.0.0",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^4.4.2",
    "vitest": "^0.32.2"
  },
  "dependencies": {
    "@cartamd/plugin-code": "^3.0.1",
    "@cartamd/plugin-emoji": "^3.1.0",
    "@stablelib/random": "^1.0.2",
    "carta-md": "^3.1.3",
    "edgedb": "^1.4.1",
    "oazapfts": "^4.11.2",
    "svelte-kit-cookie-session": "^4.0.0",
    "viem": "^1.19.11"
  },
  "type": "module"
}
